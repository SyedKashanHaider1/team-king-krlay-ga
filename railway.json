{
  "$schema": "https://railway.app/railway.schema.json",
  "build": {
    "builder": "nixpacks",
    "buildCommand": "pip install -r requirements.txt"
  },
  "deploy": {
    "numReplicas": 1,
    "startCommand": "gunicorn --worker-class=sync --workers=2 --timeout=60 --bind=0.0.0.0:$PORT 'backend.app:app'"
  }
}
